{
  "name": "postpage-ts",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc",
    "typeorm": "ts-node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:generate": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:generate -d src/utils/connect.ts",
    "migration:up": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run -d src/utils/connect.ts",
    "migration:down": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert -d src/utils/connect.ts"
  },
  "keywords": [],
  "author": "",
  "module": "module",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.0",
