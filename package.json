    "class-validator": "^0.14.0",
    "config": "^3.3.9",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.9",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.2",
    "nodemailer": "^6.9.3",
    "pg": "^8.4.0",
    "pino": "^8.14.1",
    "pino-pretty": "^10.0.1",
    "prom-client": "^14.2.0",
    "reflect-metadata": "^0.1.13",
    "response-time": "^2.3.2",
    "save-dev": "^0.0.1-security",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "0.3.17",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/config": "^3.3.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^20.4.1",
    "@types/nodemailer": "^6.4.9",
    "@types/pino": "^7.0.5",
    "@types/response-time": "^2.3.5",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
